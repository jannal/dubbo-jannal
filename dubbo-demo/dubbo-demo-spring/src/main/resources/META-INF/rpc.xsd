        <xsd:attribute name="id" type="xsd:string">
        </xsd:attribute>
        <xsd:attribute name="contact" type="xsd:string">
        </xsd:attribute>
        <xsd:attribute name="serviceName" type="xsd:string">
        </xsd:attribute>
        <xsd:attribute name="serviceImplName" type="xsd:string">
        </xsd:attribute>
    </xsd:complexType>

</xsd:schema>